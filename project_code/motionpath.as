package  {		import flash.display.MovieClip;		import com.greensock.*;	import com.greensock.motionPaths.*;	public class motionpath extends MovieClip {						public function motionpath() {			// constructor code						//create a circle motion path at coordinates x:150, y:150 with a radius of 100			var circle:Circle2D = new Circle2D(150,150,100);						//make the MovieClip "mc" follow the circle and start at a position of 90 degrees (this returns a PathFollower instance)			var follower:PathFollower = circle.addFollower(mc, circle.angleToProgress(90), true);						//tween the follower clockwise along the path to 315 degrees			TweenLite.to(follower, 2, {progress:circle.followerTween(follower, 315, Direction.CLOCKWISE)});						//tween the follower counter-clockwise to 200 degrees and add an extra revolution			TweenLite.to(follower, 2, {progress:circle.followerTween(follower, 200, Direction.COUNTER_CLOCKWISE, 1)});		}	}	}